<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/")css/style.css" />
    <script type="text/javascript" src="~/Content/js/boxOver.js"></script>
    <script src='https://www.google.com/recaptcha/api.js'></script>
    @RenderSection("head", false)

</head>
<body>
    <div id="main_container">
        <div id="headeradmin">
            <div id="logo"> <a href="@Url.Action("Index","Home")"><img src="@Url.Content("~/Content/")images/logo.png" alt="" border="0" width="237" height="140" /></a> </div>
            <div class="oferte_content">
                <div class="top_divider"><img src="@Url.Content("~/Content/")images/header_divider.png" alt="" width="1" height="164" /></div>
            </div>
            <!-- end of oferte_content-->
        </div>
        <div id="main_content">
            <div id="menu_tab">
                <div class="left_menu_corner"></div>
                <ul class="menu">
                    <li><a href="@Url.Action("Index","Product")" class="nav1"> Home</a></li>
                    <li class="divider"></li>
                    <li><a href="@Url.Action("Index","Product")" class="nav2">Products</a></li>
                    <li class="divider"></li>
                    <li class="nav11"><a href="@Url.Action("Index","OrderDetail")" >Report</a></li>
                    @if (Session["AdminLogin"] != null)
                    {
                        <li class="divider"></li>
                        <li><a href="~/AdminLogin/Logout" class="nav4">Logout</a></li>
                        <li class="divider"></li>
                    }

                    <li><a href="@Url.Action("Index","Customer")" class="nav4">Users</a></li>
                    <li class="divider"></li>
                    @*<li class="nav11">
                        Report
                        <select onchange="location = this.value;">
                            <option value="~/OrderDetail/Index">Order Summery</option>
                            <option value="#">Best Product</option>
                        </select>
                    </li>
                    <li class="divider"></li>*@
                    <li><a href="@Url.Action("Index","Admin")" class="nav4">Profile</a></li>
                 </ul>
                <div class="right_menu_corner"></div>
            </div>
            <!-- end of menu tab -->

            <div class="crumb_navigation"> Navigation: <span class="current">Home</span> </div>
            <div class="left_content">
                <div class="title_box">Dashboard</div>
                <ul class="left_menu">
                    <li class="odd"><a href="~/Category/Create">Add Category</a></li>
                    <li class="even"><a href="~/Category/Index">Edit Category</a></li>
                    <li class="odd"><a href="~/Category/Index">Delete Category</a></li>
                    <li class="odd"><a href="~/Product/AddProduct">Add Products</a></li>
                    <li class="even"><a href="~/Product/Index">Edit Products</a></li>
                    <li class="odd"><a href="~/Product/Index">Delete Products</a></li>
                    <li class="even"><a href="~/Customer/Create">Add Customer</a></li>
                    <li class="odd"><a href="~/Customer/Index">Delete Customer</a></li>
                    <li class="even"><a href="~/OrderDetail/Index">Maintain Orders</a></li>
                    <li class="odd"><a href="#">Messages</a></li>
                </ul>
            </div>
            <!-- end of left content -->

            @RenderBody()


            <!-- end of right content -->
        </div>
        <!-- end of main content -->
    </div>
</body>
</html>